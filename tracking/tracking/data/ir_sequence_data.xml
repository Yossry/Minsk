<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_route_order" model="ir.sequence">
            <field name="name">Ruta de Ordenes</field>
            <field name="code">route.order</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="product_product_discount_lx" model="product.product">
            <field name="name">Discount LX</field>
            <field name="available_in_pos">True</field>
            <field name="standard_price">1</field>
            <field name="list_price">1</field>
            <field name="type">consu</field>
            <field name="categ_id" ref="point_of_sale.product_category_pos"/>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
            <field name="default_code">DISCLX</field>
        </record>
        <record id="seq_quotation_pos_report" model="ir.sequence">
            <field name="name">quotation.pos.report</field>
            <field name="code">quotation.pos.report</field>
            <field name="prefix">QPR</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
