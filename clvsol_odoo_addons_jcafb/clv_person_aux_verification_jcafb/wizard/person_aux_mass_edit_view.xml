<odoo>

    <record id="clv_person_aux_mass_edit_view_form_verification" model="ir.ui.view">
        <field name="name">clv.person_aux.mass_edit.form</field>
        <field name="model">clv.person_aux.mass_edit</field>
        <field name="inherit_id" ref="clv_person_aux.clv_person_aux_mass_edit_view_form"/>
        <field name="priority" eval="8" />
        <field name="arch" type="xml">
            <group name="marker_ids" position="after">
                <group>
                    <field name="ref_address_is_unavailable_selection"/>
                </group>
                <group name="ref_address_is_unavailable">
                    <field name="ref_address_is_unavailable" nolabel='1'/>
                </group>
                <group>
                    <field name="ref_address_id_selection"/>
                </group>
                <group name="ref_address_id">
                    <field name="ref_address_id" options="{'no_open': False, 'no_create': True}" nolabel='1'/>
                </group>
                <group>
                    <field name="ref_address_aux_is_unavailable_selection"/>
                </group>
                <group name="ref_address_aux_is_unavailable">
                    <field name="ref_address_aux_is_unavailable" nolabel='1'/>
                </group>
                <group>
                    <field name="ref_address_aux_id_selection"/>
                </group>
                <group name="ref_address_aux_id">
                    <field name="ref_address_aux_id" options="{'no_open': False, 'no_create': True}" nolabel='1'/>
                </group>
                <group>
                    <field name="family_is_unavailable_selection"/>
                </group>
                <group name="family_is_unavailable">
                    <field name="family_is_unavailable" nolabel='1'/>
                </group>
                <group>
                    <field name="family_id_selection"/>
                </group>
                <group name="family_id">
                    <field name="family_id" options="{'no_open': False, 'no_create': True}" nolabel='1'/>
                </group>
                <group>
                    <field name="family_aux_is_unavailable_selection"/>
                </group>
                <group name="family_aux_is_unavailable">
                    <field name="family_aux_is_unavailable" nolabel='1'/>
                </group>
                <group>
                    <field name="family_aux_id_selection"/>
                </group>
                <group name="family_aux_id">
                    <field name="family_aux_id" options="{'no_open': False, 'no_create': True}" nolabel='1'/>
                </group>
                <group>
                    <field name="contact_info_is_unavailable_selection"/>
                </group>
                <group name="contact_info_is_unavailable">
                    <field name="contact_info_is_unavailable" nolabel='1'/>
                </group>
                <group>
                    <field name="clear_address_data"/>
                </group>
                <group name="clear_address_data">
                </group>
                <group>
                    <field name="verification_marker_ids_selection"/>
                </group>
                <group name="verification_marker_ids">
                    <field name="verification_marker_ids" options="{'no_open': False, 'no_create': True}" nolabel='1'/>
                </group>
                <group>
                    <field name="person_aux_verification_exec"/>
                </group>
                <group name="person_aux_verification_exec">
                </group>
            </group>
        </field>
    </record>

</odoo>
