<odoo>

    <record id="clv_family_aux_mass_edit_view_form_employee" model="ir.ui.view">
        <field name="name">clv.family_aux.mass_edit.form</field>
        <field name="model">clv.family_aux.mass_edit</field>
        <field name="inherit_id" ref="clv_family_aux.clv_family_aux_mass_edit_view_form"/>
        <field name="priority" eval="8" />
        <field name="arch" type="xml">
            <group name="global_tag_ids_selection" position="before">
                <group>
                    <field name="reg_state_selection"/>
                </group>
                <group>
                    <field name="reg_state" nolabel='1'/>
                </group>
                <group>
                    <field name="state_selection"/>
                </group>
                <group>
                    <field name="state" nolabel='1'/>
                </group>
            </group>
        </field>
    </record>

</odoo>
